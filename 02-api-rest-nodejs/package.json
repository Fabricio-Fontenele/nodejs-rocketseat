{
  "name": "typescript",
  "version": "1.0.0",
  "description": "Node, Typescript, ESLint Template Project",
  "main": "src/main.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules ./src/server.ts",
    "start": "node ./build/src/index.js",
    "build": "rimraf ./build && tsc && tsc-alias",
    "lint": "eslint .",
    "lint:clear": "rm -f .eslintcache",
    "lint:fix": "yarn run lint --fix",
    "test": "vitest"
  },
  "keywords": [],
  "author": "Eyder Rios",
  "license": "MIT",
  "resolutions": {
    "glob": "^9",
    "rimraf": "^4"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "dotenv": "^16",
    "fastify": "^5.7.1",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^22",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "eslint-import-resolver-typescript": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-markdown": "^5",
    "eslint-plugin-n": "^17",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^7",
    "lint-staged": "^15",
    "prettier": "^3",
    "ts-node": "^10",
    "ts-node-dev": "^2",
    "tsc-alias": "^1",
    "tsconfig-paths": "^4",
    "typescript": "^5",
    "typescript-transform-paths": "^3",
    "vitest": "^4.0.17"
  }
}
